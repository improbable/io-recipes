	<key>Identifier</key>
	<string>improbable.munki.xonotic-osx-sdl</string>
	<key>Input</key>
	<dict>
		<key>APP_FILENAME</key>
		<string>Xonotic</string>
		<key>MUNKI_REPO_SUBDIR</key>
		<string>apps/%NAME%</string>
		<key>NAME</key>
		<string>xonotic-osx-sdl</string>
		<key>pkginfo</key>
		<dict>
			<key>catalogs</key>
			<array>
				<string>testing</string>
			</array>
			<key>description</key>
			<string>Cross-platform first-person-shooter game.</string>
			<key>developer</key>
